{{outlet}}
<div class="container">
  <form class="form-signin {{isSubmitted}}" id="frmlogin">
    <h2 class="form-signin-heading">Please sign in</h2>
    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" id="inputEmail" required class="form-control" placeholder="Username" onchange= {{action (mut email) value= "target.value"}}
    />
    <br>
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" required class="form-control" placeholder="Password" value=password onchange= {{action (mut password) value= "target.value"}}
    />
    <div class="checkbox">
    </div>
    <button {{action "login" }} class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
  </form>
</div>